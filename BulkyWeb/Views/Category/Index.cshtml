@{
}
@model List<Category>
<h1> Category List</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Category Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model.OrderBy(c => c.DisplayOrder))
        {
        <tr>
            <td>@category.Name</td>
            <td>@category.DisplayOrder</td>
        </tr>
        }
       
    </tbody>

</table>



